{
    "name": "Eye Rest",
    "version": "1.0",
    "description": "An extension to rest your eyes every 20 minutes for 20 seconds.",
    "permissions": ["alarms", "declarativeContent", "storage"],
    "background": {
      "scripts": ["helpers.js", "background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      }
    },
    "icons": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    },
    "manifest_version": 2
  }